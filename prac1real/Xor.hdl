/**
 * Exclusive-or gate:
 * out = not (a == b)
 */

CHIP Xor {
    IN a, b;
    OUT out;

    PARTS:
    // Put your code here:
    Nand(a = a, b = b, out = NandAB);
    Nand(a = a, b = NandAB, out = HalfXor1);
    Nand(a=b, b=NandAB, out = HalfXor2);
    Nand(a = HalfXor1, b = HalfXor2, out = out);
}